<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Cleanup</servlet-name>
		<servlet-class>org.decojer.web.servlet.CleanupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DecoJerQueue</servlet-name>
		<servlet-class>org.decojer.web.servlet.DecoJerQueueServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Decompile</servlet-name>
		<servlet-class>org.decojer.web.servlet.DecompileServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FrontendChannelQueue</servlet-name>
		<servlet-class>org.decojer.web.servlet.FrontendChannelQueueServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Upload</servlet-name>
		<servlet-class>org.decojer.web.servlet.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Cleanup</servlet-name>
		<url-pattern>/cleanup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DecoJerQueue</servlet-name>
		<url-pattern>/_ah/queue/decoJer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Decompile</servlet-name>
		<url-pattern>/decompile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FrontendChannelQueue</servlet-name>
		<url-pattern>/_ah/queue/frontendChannel</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>